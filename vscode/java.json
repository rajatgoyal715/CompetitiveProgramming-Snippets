	// Place your snippets for java here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Print output": {
		"prefix": "pln",
		"body": [
			"System.out.println($1)",
		],
		"description": "Print output in next line."
	},
	"Class Declaration": {
		"prefix": "class",
		"body": [
			"class $1 {",
			"\t$0",
			"}"
		],
		"description": "Declare class"
	},
	"Main Function": {
		"prefix": "main",
		"body": [
			"public static void main(String[] args) {",
			"\t$0",
			"}",
		],
		"description": "Main function"
	},
	"Basic import statements": {
		"prefix": "imp",
		"body": [
			"import java.util.*;",
			"import java.io.*;",
			"",
			"$0",
		],
		"description": "Basic import statements",
	},
	"Scanner object declaration": {
		"prefix": "sc",
		"body":	"Scanner sc = new Scanner(System.in);\n",
		"description": "Scanner object declaration",
	},
	"BufferedReader Object declaration": {
		"prefix": "br",
		"body": "BufferedReader br = new BufferedReader(new InputStreamReader(System.in));",
		"description": "BufferedReader Object declaration",
	},
	"Integer parseInt": {
		"prefix": "pi",
		"body": "Integer.parseInt($1);\n$0",
		"description": "parseInt function"
	},
	"Long parseLong": {
		"prefix": "pl",
		"body": "Long.parseLong($1);\n$0",
		"description": "parseLong function"
	},
	"StringBuilder": {
		"prefix": "sb",
		"body": "StringBuilder ${1:sb} = new StringBuilder();\n$0",
		"description": "StringBuilder object declaration"
	}
